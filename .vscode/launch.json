    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Linux) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/sipacker_ua",
            "preLaunchTask": "rust: cargo build",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "args": ["--ip-addr", "0.0.0.0", "--port", "5060"],
            "env": {
                "RUST_LOG": "debug,neli=error",
                "RUST_BACKTRACE": "full",
            },
        },
     ]

}